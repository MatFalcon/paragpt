<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="presale_fuel_type_form_view" model="ir.ui.view">
            <field name="name">presale_fuel_type.form</field>
            <field name="model">presale.fuel_type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                           <field name="name"/>
                           <field name="consumption_rate"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="presale_fuel_type_tree_view" model="ir.ui.view">
            <field name="name">presale_fuel_type.tree</field>
            <field name="model">presale.fuel_type</field>
            <field name="arch" type="xml">
                <tree >
                    <field name="name"/>
                    <field name="consumption_rate"/>
                </tree>
            </field>
        </record>

        <record id="presale_fuel_type_search_view" model="ir.ui.view">
            <field name="name">presale_fuel_type.search</field>
            <field name="model">presale.fuel_type</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                    <field name="consumption_rate"/>
                </search>
            </field>
        </record>

        <record id="action_presale_fuel_type" model="ir.actions.act_window">
            <field name="name">Fuel Types</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">presale.fuel_type</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Create new fuelk types rates for presales projects
                </p>
            </field>
        </record>
    </data>
</odoo>