<odoo>
    <record id="sales_order_view_presale_form" model="ir.ui.view">
        <field name="name">sales.order.form.presale</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/group[@name='sale_header']/group[@name='partner_details']" position="inside">
                <field name="presale_id"/>
                <field name="es_preventa"/>
            </xpath>
            <field name="order_line" position="attributes">
                <attribute name="readonly">0</attribute>
            </field>
        </field>
    </record>
</odoo>
