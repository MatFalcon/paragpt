<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="presale_types_additional_form_view" model="ir.ui.view">
            <field name="name">presale_types_additional.form</field>
            <field name="model">presale.types_additionals</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                           <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="presale_types_additional_view" model="ir.ui.view">
            <field name="name">presale_types_additional.tree</field>
            <field name="model">presale.types_additionals</field>
            <field name="arch" type="xml">
                <tree >
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="action_presale_types_additional" model="ir.actions.act_window">
            <field name="name">Types of Additionals</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">presale.types_additionals</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Create new labor types for presales additionals
                </p>
            </field>
        </record>
    </data>
</odoo>