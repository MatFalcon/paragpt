<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="view_partner_form_inherit_mantenimiento_registro" model="ir.ui.view">
			<field name="name">view_partner_form_inherit_mantenimiento_registro</field>
			<field name="model">res.partner</field>
			<field name="inherit_id" ref="base.view_partner_form" />
			<field name="arch" type="xml">

				<xpath expr="//field[@name='vat']" position="after">
					<field name="tipo_sociedad" widget="selection" />
					<field name="capital" widget="monetary" />
				</xpath>

			</field>
		</record>

	</data>
</odoo>