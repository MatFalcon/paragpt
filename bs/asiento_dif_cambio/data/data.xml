<odoo>
    <data noupdate="1">
        <record model="ir.config_parameter" id="last_rate_compra">
            <field name="key">last_rate_compra</field>
            <field name="value"></field>
        </record>
        <record model="ir.config_parameter" id="last_rate_venta">
            <field name="key">last_rate_venta</field>
            <field name="value"></field>
        </record>

        <record forcecreate="True" id="revertir_asiento_dif_cambio_automatico" model="ir.cron">
            <field name="name">Revertir asiento por diferencia de cambio del día</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">24</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field ref="account.model_account_move" name="model_id"/>
            <field name="state">code</field>
            <field name="code">model.revertir_asiento_dif_auto()</field>
            <field eval="False" name="doall"/>
            <!--            <field name="function">True</field>-->
        </record>

        <record forcecreate="True" id="crear_asiento_dif_cambio_automatico" model="ir.cron">
            <field name="name">Crear asiento por diferencia de cambio del día</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field ref="account.model_account_move" name="model_id"/>
            <field name="state">code</field>
            <field name="code">model.create_asiento_dif_auto()</field>
            <field eval="False" name="doall"/>
            <!--            <field name="function">True</field>-->
        </record>

    </data>
</odoo>