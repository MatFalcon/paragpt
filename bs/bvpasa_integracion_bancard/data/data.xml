<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record forcecreate="True" id="bvpasa_integracion_bancard.facturar_bancard" model="ir.cron">
            <field name="name">Generar Facturas de Bancard</field>
            <field eval="False" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">24</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field ref="bvpasa_integracion_bancard.model_bvpasa_integracion_bancard" name="model_id"/>
            <field name="state">code</field>
            <field name="code">model.FacturarCoincidentes()</field>
            <field eval="False" name="doall"/>
            <!--            <field name="function">True</field>-->
        </record>


         <record forcecreate="True" id="bvpasa_integracion_bancard.obtener_reportes_bancard" model="ir.cron">
            <field name="name">Obtener reportes de Bancard</field>
            <field eval="False" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">24</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field ref="bvpasa_integracion_bancard.model_bvpasa_integracion_bancard" name="model_id"/>
            <field name="state">code</field>
            <field name="code">model.obtenerReportes()</field>
            <field eval="False" name="doall"/>
            <!--            <field name="function">True</field>-->
        </record>

        <record forcecreate="True" id="bvpasa_integracion_bancard.obtener_datos_campus" model="ir.cron">
            <field name="name">Obtener datos del Campus Virtual</field>
            <field eval="False" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">24</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field ref="bvpasa_integracion_bancard.model_bvpasa_integracion_campus" name="model_id"/>
            <field name="state">code</field>
            <field name="code">model.getRecords()</field>
            <field eval="False" name="doall"/>
            <!--            <field name="function">True</field>-->
        </record>


