<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_partner_form_custodia_fisica_inherit" model="ir.ui.view">
            <field name="name">res.partner.form.custodia.fisica.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="arancel_cantidad_id" />
                    <field name="arancel_monto_id" />
                </xpath>
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="geene_api_pk" groups="base.group_system" />
                </xpath>
            </field>
        </record>

    </data>
</odoo>