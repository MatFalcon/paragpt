<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="custodia_fisica_product_view_form_inherit" model="ir.ui.view">
            <field name="name">custodia.fisica.product.view.form.inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='barcode']" position="after">
                    <field name="es_custodia_fisica" />
                    <field name="geene_api_pk" groups="base.group_system" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>