<odoo>
    <data noupdate="1">
        <record id="cron_obtener_lineas_custodia" model="ir.cron">
            <field name="name">Obtener Lineas de Custodia API</field>
            <field name="model_id" ref="model_custodia_fisica_custodia_fisica_line" />
            <field name="user_id" ref="base.user_root" />
            <field name="active" eval="False" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="state">code</field>
            <field name="priority">5</field>
            <field name="numbercall">-1</field>
            <field name="code">model.get_vals_api()</field>
        </record>
        <record id="cron_procesar_sin_obtener_custodias_importadas" model="ir.cron">
            <field name="name">Procesar Sin Obtener Lineas Custodias Importadas </field>
            <field name="model_id" ref="model_custodia_fisica_custodia_fisica_line" />
            <field name="user_id" ref="base.user_root" />
            <field name="active" eval="False" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="state">code</field>
            <field name="priority">5</field>
            <field name="numbercall">-1</field>
            <field name="code">model.create_lineas_custodia(add_import=False)</field>
        </record>
        <record id="cron_procesar_custodias_importadas" model="ir.cron">
            <field name="name">Procesar Custodias Importadas </field>
            <field name="model_id" ref="model_custodia_fisica_custodia_fisica_line" />
            <field name="user_id" ref="base.user_root" />
            <field name="active" eval="False" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="state">code</field>
            <field name="priority">5</field>
            <field name="numbercall">-1</field>
            <field name="code">model.create_lineas_custodia()</field>
        </record>
    </data>
</odoo>