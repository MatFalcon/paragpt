<odoo>
    <data>

                <!--Reporte de la Caja POS-->
    <report
            id="report_caja_pos_py"
            model="pos.session"
            string="Caja"
            report_type="qweb-pdf"
            name="pos_paraguay.caja_report"
            file="pos_paraguay.caja_report"
            paperformat="base.paperformat_euro"
            print_report_name="'Caja-%s' % (object.name)"
    />

        <record id="paperformat_factura_ticket2" model="report.paperformat">
        <field name="name">ticket</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">250</field>
        <field name="page_width">76</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">10</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">4.50</field>
        <field name="margin_right">4.50</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">20</field>
        <field name="dpi">90</field>
    </record>

    <!--FACTURA PRODUCTO-->

    <report
        id="factura_pos_boton_2"
        model="account.move"
        string="Factura Ticket POS"
        name="pos_paraguay.report_factura_2"
        file="pos_paraguay.report_factura_2"
        paperformat="pos_paraguay.paperformat_factura_ticket2"
        report_type="qweb-pdf"
        print_report_name="'Ticket - %s' % (object.nro_factura)"
        />
    </data>





</odoo>