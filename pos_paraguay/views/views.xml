                    </field>
        </record>

       <record model="ir.ui.view" id="pos_paraguay_config_form_inherited">
                    <field name="name">pos.config.form.inh</field>
                    <field name="model">res.config.settings</field>
                    <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form" />
                    <field name="arch" type="xml">
                        <xpath expr="//setting[@id='pos_default_journals']" position="after">

                          <setting string="Talonarios" id="pos_default_talonarios" help="Talonarios por Defecto">
                                <div class="content-group mt16">
                            <div class="row">
                                <label string="Facturas " for="pos_talonario_factura" class="col-md-3 o_light_label"/>
                              <field name="pos_talonario_factura" domain="[('tipo_documento','=','1'),('activo','=',True)]"/>
                             </div>
                            <div class="row">
                             <label string="Nota de Credito" for="pos_talonario_nota_credito" class="col-md-3 o_light_label"/>
                                <field name="pos_talonario_nota_credito" domain="[('tipo_documento','=','4'),('activo','=',True)]"/>
                            </div>
                            <!--<div class="row">-->
                            <!--<label string="Formato Factura" for="pos_invoice_report" class="col-md-3 o_light_label"/>-->

                            <!--<field name="pos_invoice_report"/>-->
                            <!--</div>-->
                            </div>
                            </setting>
<!--                            <field name="invoice_report_text" invisible="1"/>-->

                            <!-- <field name="nombrefantasia"/> -->

                           <!--  <field name="efectivo_tesoreria"  />

                            <field name="efectivo_tesoreria_usd"  domain="[('currency_id','=',3)]" /> -->
                        </xpath>


                    </field>
        </record>

