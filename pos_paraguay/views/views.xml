     <record model="ir.ui.view" id="pos_paraguay_company_form_inherited">
                    <field name="name">pos.base_company.form.inh</field>
                    <field name="model">res.company</field>
                    <field name="inherit_id" ref="base.view_company_form"/>
                    <field name="arch" type="xml">

                        <xpath expr="//field[@name='website']" position="after">
                            <field name="invoice_report" />
                            <field name="invoice_report_text" invisible="1"/>

                            <!-- <field name="nombrefantasia"/> -->

