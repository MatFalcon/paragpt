<odoo>
 <record id="view_invoice_form_inh" model="ir.ui.view">
            <field name="name">view.fact.caja.chica</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
		        <xpath expr="//header" position="inside">
                    <button name="action_print_custom_ticket"
                            type="object"
                            string="Imprimir Ticket"
                            class="oe_highlight"
                            icon="fa-print"/>
                </xpath>
            </field>
        </record>

</odoo>